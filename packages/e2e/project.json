{
  "name": "e2e",
  "root": "packages/e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["cli", "dashboard"],
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vitest run --passWithNoTests",
        "cwd": "packages/e2e"
      },
      "cache": false,
      "dependsOn": [
        {
          "projects": "cli",
          "target": "build"
        },
        {
          "projects": "dashboard",
          "target": "build"
        }
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vitest run --passWithNoTests",
        "cwd": "packages/e2e"
      },
      "inputs": ["default"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No lint configured for e2e package'",
        "cwd": "packages/e2e"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No build for pure-test e2e package'",
        "cwd": "packages/e2e"
      }
    }
  }
}
